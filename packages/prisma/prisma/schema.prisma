generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  password String
}

enum QType {
  Remember
  Understand
  Analyze
  Apply
  Create
}

enum CO {
  CO1
  CO2
  CO3
  CO4
  CO5
}

model Unit1 {
  unitId      Int    @id @default(autoincrement())
  parta1      String @unique
  parta1qtype QType
  parta1co    CO
  parta2      String @unique
  parta2qtype QType
  parta2co    CO
  partb1a      String @unique
